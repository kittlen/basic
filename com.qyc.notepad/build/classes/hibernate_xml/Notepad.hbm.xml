<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="model">
	<class name="Notepad" table="T_Notepad">
		<id name="id" column="id" type="string">
			<generator class="assigned"></generator>
		</id>
		<property name="title" column="title" type="text"></property>
		<property name="content" column="nContent" type="text"></property>
		<property name="date" column="date" type="string"
			length="50"></property>
		<property name="type" column="type" type="integer"></property>
	</class>
</hibernate-mapping>
